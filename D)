		{
			 string texto = "";

        string mensagem = (texto.Length == 0) ? "A string está vazia." : "A string contém texto.";

        Console.WriteLine(mensagem);

        Console.WriteLine(mensagem);
			
			Console.Write("Press any key to continue . . . ");
